      "jinja": true,
      "justMyCode": true,
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      // add config for env variable
      "env": {
        "TGB_FLASK_APP": "app.py",
        "TGB_FLASK_ENV": "development"
      }
    }
  ]
